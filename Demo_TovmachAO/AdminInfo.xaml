<Window x:Class="Demo_TovmachAO.AdminInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Demo_TovmachAO"
        mc:Ignorable="d"
        Title="AdminInfo" Height="450" Width="800">
    <Grid>
        <TextBlock Style="{StaticResource TitleStyle}" Text="Страница старшего продавца" FontSize="30"/>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
            <Button Content="Список заказов" x:Name="OrdersButton" Click="OrdersButton_Click" FontSize="25"/>
            <Button Margin="100,0,0,0" Content="Добавить клиента" x:Name="ClientsButotn" Click="ClientsButotn_Click" FontSize="25"/>
        </StackPanel>
    </Grid>
</Window>
